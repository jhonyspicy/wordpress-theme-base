{
	"name": "jhonyspicy/wordpress-theme-base-template",
	"description": "This makes easier to make WordPress theme",
	"type": "wordpress-theme",
	"keywords": ["wordpress", "theme", "template"],
	"version":  "0.2.1",
	"repositories": [
		{
			"type": "composer",
			"url": "http://wpackagist.org"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"jhonyspicy/wordpress-theme-base": "*",
		"atomita/php-utils": "*",
		"atomita/wordpress-utils": "*",
		"wpackagist-plugin/wp-multibyte-patch": "*",
		"wpackagist-plugin/force-regenerate-thumbnails": "2.0.*",
		"atomita/wordpress-thumbnail-filename-changer": "0.1.*",
		"twig/twig": "1.*"
	},
	"autoload": {
		"psr-0": {"": "classes/"}
	},
	"authors":[
		{
			"name": "Jhony Spicy"
		},
		{
			"name": "atomita",
			"homepage": "https://atomita.github.io/"
		}
	],
	"extra": {
		"installer-paths": {
			"../../plugins/{$name}/": ["type:wordpress-plugin"]
		}
	}
}
